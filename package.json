{
  "name": "@jtbales/alpaca-trade-api-ts",
  "version": "5.1.5",
  "description": "A TypeScript Node.js library for the https://alpaca.markets REST API and WebSocket streams.",
  "author": "johntbales",
  "contributors": [
    "lbstr",
    "AqilCont",
    "KalebMills",
    "anonrose",
    "husayt"
  ],
  "license": "ISC",
  "keywords": [
    "alpaca",
    "alpaca-trade-api-js",
    "alpaca trade api",
    "trading",
    "exchange",
    "bot",
    "alpaca markets",
    "markets",
    "stocks",
    "stock"
  ],
  "type": "module",
  "types": "./@types/index.d.ts",
  "typings": "./@types/index.d.ts",
  "unpkg": "./dist/alpaca.js",
  "jsdelivr": "./dist/alpaca.js",
  "browser": "./dist/alpaca.browser.min.js",
  "main": "./dist/mjs/index.js",
  "module": "./dist/mjs/index.js",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.cjs",
    "default": "./dist/alpaca.browser.min.js"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist/",
    "@types/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jtbales/alpaca-trade-api-ts.git"
  },
  "scripts": {
    "build": "npm run clean && npm i && npm run build:cjs:only",
    "build:cjs:only": "tsc --outDir dist/ --esModuleInterop --moduleResolution node  --module CommonJS --target ES2015 -d --declarationDir @types",
    "build:all": "npm run clean && npm i && npm run build:mjs && npm run build:cjs && npm run build:esm && npm run build:browser && npm run build:esm_bundle",
    "build:mjs": "tsc --outDir dist/mjs --allowSyntheticDefaultImports -d --declarationDir @types --module esnext --target esnext",
    "build:cjs": "tsc --outDir dist/cjs --esModuleInterop --moduleResolution node  --module CommonJS --target ES2015 -d false && map dist/cjs js cjs",
    "build:esm": "rollup -c conf/rollup/dist/esm.js",
    "build:esm_bundle": "rollup -c conf/rollup/dist/esm_bundle.js",
    "build:browser": "rollup -c conf/rollup/dist/browser.js",
    "clean": "rimraf dist @types",
    "test": "npm run test:default",
    "test:default": "jest",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "dependencies": {
    "@master-chief/map": "^0.0.6",
    "abort-controller": "^3.0.0",
    "bottleneck": "^2.19.5",
    "bufferutil": "^4.0.3",
    "eventemitter3": "^4.0.7",
    "isomorphic-unfetch": "^3.1.0",
    "isomorphic-ws": "^4.0.1",
    "qs": "^6.9.6",
    "utf-8-validate": "^5.0.4",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/jest": "^26.0.20",
    "@types/node-fetch": "^2.5.8",
    "@types/qs": "^6.9.5",
    "@types/ws": "^7.4.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.5.1",
    "tslib": "^2.0.3",
    "typescript": "^4.2.1-rc"
  },
  "bugs": {
    "url": "https://github.com/jtbales/alpaca-trade-api-ts/issues"
  },
  "homepage": "https://github.com/jtbales/alpaca-trade-api-ts#readme",
  "directories": {
    "example": "example",
    "test": "test"
  }
}
